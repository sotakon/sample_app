<div>  
  <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
    <div><%= f.label :email, "メールアドレス" %></div>
    <div><%= f.email_field :email %></div>
    <div><%= f.label :password, "パスワード" %></div>
    <div><%= f.password_field :password %></div>
    <div><%= f.submit "ログイン" %></div>
  <% end %>
</div>